##target_extended_remote##
set mem inaccessible-by-default ${mem.access}
mon ondisconnect ${ondisconnect}
set arm force-mode thumb
${load}</script>
   <script type="run" silent="false" emulators="${gdb.stub} -mi -info-emu">${run}</script>
